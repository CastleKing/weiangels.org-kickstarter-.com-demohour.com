<?xml version="1.0" encoding="utf-8" ?>
<templete>
    <!--邮件中显示的发送人地址，非实际地址-->
    <sendEmailAddress>no-reply@weiangels.org</sendEmailAddress>
	
    <!--邮件中显示的发送人名称，非实际名称-->
    <sendUserName>微天使</sendUserName>
	
	<!--邮件是否为Html格式-->
	<isBodyHtml>true</isBodyHtml>
	
	<!--邮件主题-->
    <subject>
        <![CDATA[
        您重新设置了密码
        ]]>
    </subject>
	
	<!--邮件内容-->
    <content>
        <![CDATA[
		<P>Hi ${nickName}<p>
		<p>您在微天使的登录密码已经重新设置成功了。</p>
		<p>如果您没有这种变化的要求，请联系我们。</p>
        ]]>
    </content>
    
    <!--模板附件，填写附件的物理地址，每个附件对应一个attachment节点-->
	<!--
    <attachments>
        <attachment fileName="d:/attachement/testdoc.doc"/>
    </attachments>
	-->
</templete>